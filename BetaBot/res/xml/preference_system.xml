        android:dialogMessage="List of the axes to be used"
        android:inputType="text" />
	<com.nwags.BetaBot.MyEditFloatTextPreference
        android:title="Pipette Empty Position"
        android:key="nwae"
        android:summary="%s"
        android:defaultValue="18"
