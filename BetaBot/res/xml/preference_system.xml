<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    
	<!--	
	https://github.com/synthetos/TinyG/wiki/TinyG-Configuration
		
		"fb", "float" - no
		"fv", "float" - no
		"hv", "int"	
		"id", "string"	- no
		"ja", "float" 
		"ct", "float" 
		"st", "int"
		
		"mt", "int" - added
		
		"ej", "boolean" 
		"jv", "int" 
		"tv", "int" 
		"qv", "int" 
		"sv", "int" 
		"si", "int"
		"ic", "boolean" 
		"ec", "boolean" 
		"ee", "boolean" 
		"ex", "int"
		
		"gpl", "int"
		"gun", "boolean"
		"gco", "int"
		"gpa", "int"
		"gdi", "int	
	 -->
	
	<com.nwags.BetaBot.MyEditFloatTextPreference
		android:title="Hardware Version"
        android:key="hv"
        android:summary="%s"
        android:defaultValue="8.00"
        android:dialogTitle="Hardware Version"
        android:dialogMessage="Hardware Version"
        android:inputType="number" />
	<com.nwags.BetaBot.MyEditFloatTextPreference
        android:title="Junction acceleration"
        android:key="ja"
        android:summary="%s"
        android:defaultValue="100000"
        android:dialogTitle="Junction acceleration"
        android:dialogMessage="Global cornering acceleration value"
        android:inputType="number" />
	<com.nwags.BetaBot.MyEditFloatTextPreference
        android:title="Chordal tolerance"
        android:key="ct"
        android:summary="%s"
        android:defaultValue="0.001"
        android:dialogTitle="Chordal tolerence"
        android:dialogMessage="Sets precision of arc drawing. Trades off precision for max arc draw rate"
        android:inputType="number" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Switch type"
        android:key="st"
        android:summary="%s"
        android:defaultValue="1"
        android:dialogTitle="Switch type"
        android:dialogMessage="0=NO, 1=NC"
        android:inputType="number" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Motor disable timeout"
        android:key="mt"
        android:summary="%s"
        android:defaultValue="60"
        android:dialogTitle="Motor disable timeout"
        android:dialogMessage="Number of seconds before motor power is automatically released."
        android:inputType="number" />
	<SwitchPreference
        android:key="ej"
        android:title="Enable JSON mode"
        android:summary="0=text mode, 1=JSON mode"
        android:defaultValue="false" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="JSON verbosity"
        android:key="jv"
        android:summary="%s"
        android:defaultValue="5"
        android:dialogTitle="JSON verbosity"
        android:dialogMessage="0=silent\n1=footer\n2=messages\n3=configs\n4=linenum\n5=verbose"
        android:inputType="number" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Text mode verbosity"
        android:key="tv"
        android:summary="%s"
        android:defaultValue="1"
        android:dialogTitle="Text mode verbosity"
        android:dialogMessage="0=silent\n1=verbose"
        android:inputType="number" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Queue report verbosity"
        android:key="qv"
        android:summary="%s"
        android:defaultValue="2"
        android:dialogTitle="Queue report verbosity"
        android:dialogMessage="0=off\n1=filtered\n2=verbose"
        android:inputType="number" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Status report verbosity"
        android:key="sv"
        android:summary="%s"
        android:defaultValue="1"
        android:dialogTitle="Status report verbosity"
        android:dialogMessage="0=off\n1=filtered\n2=verbose"
        android:inputType="number" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Status report interval"
        android:key="si"
        android:summary="%s"
        android:defaultValue="250"
        android:dialogTitle="Status report interval"
        android:dialogMessage="in milliseconds (50 ms minimum interval)"
        android:inputType="number" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Ignore CR / LF on RX"
        android:key="ic"
        android:summary="%s"
        android:defaultValue="0"
        android:dialogTitle="Ignore CR / LF on RX"
        android:dialogMessage="0=accept CR or LF as line terminator\n1=ignore CRs\n2=ignore LFs"
        android:inputType="number" />
	<SwitchPreference
        android:key="ec"
        android:title="Enable CR on TX"
        android:summary="0=send LF line termination on TX\n1= send both LF and CR termination"
        android:defaultValue="false" />
	<SwitchPreference
        android:key="ee"
        android:title="Enable character echo"
        android:summary="0=off, 1=enabled"
        android:defaultValue="false" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Enable flow control"
        android:key="ex"
        android:summary="%s"
        android:defaultValue="0"
        android:dialogTitle="Enable flow control"
        android:dialogMessage="Enable flow control\n0=off\n1=XON/XOFF enabled\n2=RTS/CTS enabled"
        android:inputType="number" />
	
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Default plane selection"
        android:key="gpl"
        android:summary="%s"
        android:defaultValue="0"
        android:dialogTitle="Default plane selection"
        android:dialogMessage="Default plane selection\n0=XY plane (G17)\n1=XZ plane (G18)\n2=YZ plane (G19)"
        android:inputType="number" />
	<SwitchPreference
        android:key="gun"
        android:title="Default units mode"
        android:summary="0=inches mode (G20)\n1=mm mode (G21)"
        android:defaultValue="true" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Default coordinate system"
        android:key="gco"
        android:summary="%s"
        android:defaultValue="1"
        android:dialogTitle="Default coordinate system"
        android:dialogMessage="Default coordinate system\n1=G54, 2=G55, 3=G56\n4=G57, 5=G58, 6=G59"
        android:inputType="number" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Default path control mode"
        android:key="gpa"
        android:summary="%s"
        android:defaultValue="2"
        android:dialogTitle="Default path control mode"
        android:dialogMessage="Default path control mode\n0=Exact path mode (G61)\n1=Exact stop mode (G61.1)\n2=Continuous mode (G64)"
        android:inputType="number" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Default distance mode"
        android:key="gdi"
        android:summary="%s"
        android:defaultValue="0"
        android:dialogTitle="Default distance mode"
        android:dialogMessage="Default distance mode\n0=Absolute mode (G90)\n1=Incremental mode (G91)"
        android:inputType="number" />
	<com.nwags.BetaBot.MyEditTextPreference
        android:title="Axes"
        android:key="nwa"
        android:summary="%s"
        android:defaultValue="XYZA"
